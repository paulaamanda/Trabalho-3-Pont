//QUESTÃO 5
let codigo = prompt ("Digite o codigo do produto e veja a classificação dele")
if(codigo == 1){
	document.write("Esse codico é referente a: alimento não-perecível");
}else if((codigo >=2) && (codigo <= 4)){
	document.write("Esse codico é referente a: alimento perecível");
}else if((codigo >=5) && (codigo <= 6)){
	document.write("Esse codico é referente a: vestuário");
}else if(codigo ==7){
	document.write("Esse codico é referente a: higiene Pessoal");
}else if((codigo >=8) && (codigo <= 15)){
	document.write("Esse codico é referente a: limpeza e Utensílios Domésticos")
}else{
	document.write("Esse código é Inválido")
}
