//index.js
var nota1 = 6
var nota2 = 10
var nota3 = 9
var nota4 = 10
var media = (nota1+nota2+nota3+nota4)/4

if(media<=10 && media>=6){
	document.write ("Aprovado, media total: " + media)
}
if(media<=5 && media>=0){
	document.write ("reprovado, media total: " + media)
}
